<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.thinkequip.bizfw.auth.model.Menu" table="bf_menu">
		<id name="idBfMenu" column="id_bf_menu">
			<generator class="uuid.hex" />
		</id>
		<property name="createDate" column="create_date" />
		<property name="createBy" column="create_by" />
		<property name="updateDate" column="update_date" />
		<property name="updateBy" column="update_by" />
		<property name="name" column="name" />
		<property name="url" column="url" />
		<property name="orders" column="orders" />
		<property name="parentMenuId" column="parent_menu_id" />
	</class>
</hibernate-mapping>  